{
  "name": "kontexted",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "dev:webapp": "pnpm --filter @kontexted/webapp dev",
    "start:webapp": "pnpm --filter @kontexted/webapp start",
    "build:webapp": "pnpm --filter @kontexted/webapp build",
    "dev:collab": "pnpm --filter @kontexted/collab dev",
    "start:collab": "pnpm --filter @kontexted/collab start",
    "db:migrate": "pnpm --filter @kontexted/webapp db:migrate"
  }
}
